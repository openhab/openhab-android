{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended",
        "schedule:weekly"
    ],
    "minimumReleaseAge": "7 days",
    "packageRules": [
        {
            "matchPackageNames": [
                "com.google.firebase-firebase-bom"
            ],
            "enabled": false
        }
    ],
    "labels": [
        "dependencies"
    ],
    "configMigration": true
}
