<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="HABDroid.Light" parent="@style/Theme.AppCompat.Light">
        <item name="customMicIcon">@android:drawable/ic_btn_speak_now</item>
        <item name="widgetlistItemDivider">?android:dividerHorizontal</item>
        <item name="colorPrimary">#FF5722</item>
        <item name="colorAccent">#FF5722</item>
        <item name="colorPrimaryDark">#BF360C</item>
        <item name="frameWidgetDividerBackground">@drawable/material_divider</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
    </style>
    <style name="HABDroid.Dark" parent="@style/Theme.AppCompat">
        <item name="customMicIcon">@android:drawable/ic_btn_speak_now</item>
        <item name="widgetlistItemDivider">?android:dividerHorizontal</item>
        <item name="colorPrimary">#9E9E9E</item>
        <item name="colorAccent">#9E9E9E</item>
        <item name="colorPrimaryDark">#212121</item>
        <item name="frameWidgetDividerBackground">@drawable/material_divider_dark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
    </style>
</resources>
